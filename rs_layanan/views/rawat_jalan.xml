<odoo>
    <data>
        <!-- list View -->
        <record id="view_rawat_jalan_list" model="ir.ui.view">
            <field name="name">cdn.rawat_jalan.list</field>
            <field name="model">cdn.rawat_jalan</field>
            <field name="arch" type="xml">
                <list string="Rawat Jalan">
                    <field name="no_rawat"/>
                    <field name="kd_dokter"/>
                    <field name="no_rkm_medis"/>
                    <field name="pasien"/>
                </list>
            </field>
        </record>

        <!-- Form View -->
        <record id="view_rawat_jalan_form" model="ir.ui.view">
            <field name="name">cdn.rawat_jalan.form</field>
            <field name="model">cdn.rawat_jalan</field>
            <field name="arch" type="xml">
                <form string="Rawat Jalan">
                    <sheet>
                        <group>
                            <field name="no_rawat"/>
                    <field name="kd_dokter"/>
                    <field name="no_rkm_medis"/>
                    <field name="pasien"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- Search View -->
        <record id="view_rawat_jalan_search" model="ir.ui.view">
            <field name="name">cdn.rawat_jalan.search</field>
            <field name="model">cdn.rawat_jalan</field>
            <field name="arch" type="xml">
                <search string="Rawat Jalan">
                   <field name="no_rawat"/>
                    <field name="kd_dokter"/>
                    <field name="no_rkm_medis"/>
                    <field name="pasien"/>
                    <!-- <filter string="Draft" name="draft" domain="[('state', '=', 'draft')]"/>
                    <filter string="Confirmed" name="confirmed" domain="[('state', '=', 'confirmed')]"/> -->
                </search>
            </field>
        </record>

        <!-- Action -->
        <record id="action_rawat_jalan" model="ir.actions.act_window">
            <field name="name">Rawat Jalan</field>
            <field name="res_model">cdn.rawat_jalan</field>
            <field name="view_mode">list,form</field>
        </record>

        <!-- Menu -->
        <menuitem id="menu_rawat_jalan_root" name="Rawat Jalan" sequence="10"/>
        <menuitem id="menu_rawat_jalan" name="Rawat Jalan" parent="menu_rawat_jalan_root" action="action_rawat_jalan" sequence="10"/>
    </data>
</odoo>
